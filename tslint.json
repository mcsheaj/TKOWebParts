{
  "rules": {
    "class-name": true,
    "comment-format": [
        false,
        "check-space"
    ],
    "curly": true,
    "eofline": false,
    "indent": [true],
    "jsdoc-format": true,
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-arg": true,
    "no-consecutive-blank-lines": [false],
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-trailing-whitespace": false,
    "no-var-keyword": true,
    "quotemark": [true],
    "typedef-whitespace": [false],
    "semicolon": [true],
    "whitespace": [false, "check-branch", "check-decl", "check-operator", "check-module", "check-separator",
      "check-rest-spread", "check-type", "check-typecast", "check-type-operator", "check-preblock"]
  }
}

